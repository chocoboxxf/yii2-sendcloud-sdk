  "homepage": "https://github.com/chocoboxxf/yii2-sendcloud-sdk",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/chocoboxxf/yii2-sendcloud-sdk/issues",
    "wiki": "https://github.com/chocoboxxf/yii2-sendcloud-sdk/wiki",
    "source": "https://github.com/chocoboxxf/yii2-sendcloud-sdk"
  },
  "authors": [
    {
      "name": "chocoboxxf",
      "email": "neil.xuxf@gmail.com",
      "homepage": "https://github.com/chocoboxxf",
      "role": "Developer"
    }
  ],
  "require": {
